import test.assert_eq

export tests =
  test_for_loop: ||
    a = 1, 2, 3
    z = []
    for x in a
      z.push x
    assert_eq z [1 2 3]

  test_get: ||
    x = 1, 2, 3
    assert_eq (x.get 0) 1
    assert_eq (x.get 2) 3
    assert_eq (x.get 4) ()

  test_iter: ||
    x = 1, 2, 3
    assert_eq x.iter().transform(|n| "{}".format n).to_tuple() ("1", "2", "3")

  test_size: ||
    assert_eq (1, 2).size() 2
    assert_eq (1, 2, 3).size() 3
    assert_eq ((1, 2), (3, 4)).size() 2
