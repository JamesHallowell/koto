import env test.assert_eq

enumerate = |xs| xs.iter().enumerate()

export main = ||
  n = match env.args.get 0
    () then 10
    arg then arg.to_number()

  a = []
  for i in 0..n
    a = (enumerate 10..20).to_tuple()

export tests =
  test_it_works: ||
    x = (enumerate 1..=3).to_tuple()
    assert_eq x ((0, 1), (1, 2), (2, 3))
