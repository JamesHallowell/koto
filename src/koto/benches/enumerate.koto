import env test.assert_eq

enumerate = |xs| [[i x] for i, x in 0..xs.size(), xs]

export main = ||
  n = match env.args.get 0
    () then 10
    arg then arg.to_number()

  a = []
  for i in 0..n
    a = [[c d] for c, d in (enumerate 10..20)]

export tests =
  test_it_works: ||
    x = enumerate 1..=3
    assert_eq x [[0 1] [1 2] [2 3]]
