r = 0..5
assert_eq 5, size r

# ..= creates an inclusive range
y = 0..=5
assert_eq 6, size y

# Ranges are resolved when inserted in a list
assert_eq 10, size [10..20]
assert_eq 5, size [r]

# Indexing lists with ranges produces sub-lists
z = |n| n
x = [(z 10)..=(z 20)]
y = x[z(5)..(10 + 0)]
assert_eq 15, y[0]
assert_eq 5, size y
