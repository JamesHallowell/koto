o = { min: 0, max: || 42 }
assert_eq(42, o.max())
o2 = o
assert_eq(o, o2)

sum = 0
for i in o.min..o.max()
  sum = sum + i
assert_eq(sum, 861)

m =
  foo: 42
  bar: |x| x * x
  baz:
    foo: 99

assert_eq(m.foo, 42)
assert_eq(m.bar(9), 81)
assert_eq(m.baz.foo, 99)

m2 = m + { baz: 99 }
assert (m2.bar(2) == 4) and (m2.baz == 99)

assert_eq(list.sort map.keys m, ["bar", "baz", "foo"])
