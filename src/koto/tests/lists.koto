z = [10, 10 + 10, 30]
assert_eq z[0], 10
assert_eq z[0 + 1], 20

assert_eq z, z
assert_ne z, []

assert_eq z + z, [10, 20, 30, 10, 20, 30]

y = list.add(z, 40, 50)
assert_eq y[3], 40

assert_eq 5, length y
assert_eq 3, length z

a, b, c = [10, 20, 30, 40]
assert a == 10 and b == 20 and c == 30

a, b, c = [10, 20], [30, 40]
assert_eq a, [10, 20]
assert_eq b, [30, 40]
assert_eq (length c), 0

# Modifying a list
a = [0, 1, 2]
b = a
a[1], a[2] = -42, 99
a[0] = 42
assert_eq a, [42, -42, 99]
a[1..3] = 0
assert_eq a, [42, 0, 0]
# b remains unmodified
assert_eq b, [0, 1, 2]

assert list.is_sortable [42, 10, 9]
assert_eq false, list.is_sortable ["42", 10, 9] # TODO not
assert list.is_sortable ["42", "10", "9"]
assert_eq [9, 10, 42], list.sort [42, 10, 9]

