z = [10, 10 + 10, 30]
assert_eq z[0], 10
assert_eq z[0 + 1], 20

assert_eq z, z
assert_ne z, []

assert_eq z + z, [10, 20, 30, 10, 20, 30]

y = list.add(z, 40, 50)
assert_eq y[3], 40

assert_eq 5, length y
assert_eq 3, length z

a, b, c = [10, 20, 30, 40]
assert a == 10 and b == 20 and c == 30

